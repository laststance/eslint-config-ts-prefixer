{
  "name": "eslint-config-ts-prefixer",
  "author": "Ryota Murakami <dojce1048@gmail.com> (https://ryota-murakami.github.io/)",
  "license": "MIT",
  "version": "4.1.1",
  "description": "Ruleset of meaningful Lint rules on runtime with import organization. (eslint-plugin-import)",
  "type": "module",
  "main": "eslint.config.mjs",
  "files": [
    "eslint.config.mjs"
  ],
  "scripts": {
    "lint": "eslint . -c eslint.config.mjs",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "prettier": "prettier --ignore-unknown --write .",
    "prepare": "husky",
    "push-release-commit": "push-release-commit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laststance/eslint-config-ts-prefixer"
  },
  "bugs": {
    "url": "https://github.com/laststance/eslint-config-ts-prefixer/issues"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "homepage": "https://github.com/laststance/eslint-config-ts-prefixer",
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^17.1.0",
    "typescript-eslint": "^8.54.0"
  },
  "devDependencies": {
    "@laststance/npm-publish-tool": "^1.6.13",
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.0.10",
    "all-contributors-cli": "^6.26.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.2"
  }
}
