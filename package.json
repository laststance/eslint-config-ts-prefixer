{
  "name": "eslint-config-ts-prefixer",
  "author": "Ryota Murakami <dojce1048@gmail.com> (https://ryota-murakami.github.io/)",
  "license": "MIT",
  "version": "2.0.0",
  "description": "Ruleset of meaningful Lint rules on runtime and beautiful formatters. (prettier & eslint-plugin-import & eslint-plugin-sort-keys-custom-order)",
  "type": "module",
  "bin": "./bin/cli.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "scripts": {
    "lint": "eslint index.cjs index.mjs bin/cli.mjs",
    "lint:fix": "eslint index.cjs index.mjs bin/cli.mjs --fix",
    "prettier": "prettier --ignore-unknown --write .",
    "release": "release-it",
    "prepare": "husky",
    "deploy": "gh-pages -d website",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laststance/eslint-config-ts-prefixer"
  },
  "bugs": {
    "url": "https://github.com/laststance/eslint-config-ts-prefixer/issues"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "homepage": "https://github.com/laststance/eslint-config-ts-prefixer",
  "dependencies": {
    "@inquirer/prompts": "^5.0.0",
    "commander": "^12.0.0",
    "inquirer": "^10.0.1",
    "strip-json-comments": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@types/eslint": "^8.44.6",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "all-contributors-cli": "^6.26.1",
    "eslint": "^9.25.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "gh-pages": "^6.1.1",
    "globals": "^16.0.0",
    "husky": "^9.0.1",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "release-it": "^17.0.0",
    "typescript": "^5.2.2",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@typescript-eslint/parser": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "eslint": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.0.0 || ^5.0.1",
    "prettier": "^2.0.0 || ^3.0.3",
    "typescript": "^5.0.2"
  },
  "eslintConfig": {
    "extends": "./index.mjs"
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "volta": {
    "node": "22.15.0"
  }
}
